<%- model_class = @user.class -%>
<div class="row">
<div class="span8">
<h1>Your Silly Daddies</h1>
	<ul class="photos">
 	<% @photos.each do |photo| %>
    	<li>
      	  <div class="row" id="photoinfo">
		<div class="span1">
		  <div id="picvotes<%= photo.id %>" style="text-align:center">
		    <h4>Silly Score:</h4>
		    <h4><%= photo.votes.count %></h4>
		  </div>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              photo_path(photo),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>		 
		</div>
		<div class="span7" id="photo">
		  <%= link_to image_tag(photo.image.url), photo %>
		  <h3><%= photo.content %></h3>
      		</div>
	  </div>
    	</li>
  	<% end %>
	</ul>
</div>
<div class="span4">
<%= render 'static_pages/sidebar' %>
</div>
</div>
